    "class Solution:\n",
    "    def maxProfit(self, prices: List[int]) -> int:\n",
    "        cost , profit = float('+inf') , 0\n",
    "        for price in prices:\n",
    "            cost = min( cost  , price)\n",
    "            profit = max( profit , price - cost)\n",
    "        return profit"
   ]
